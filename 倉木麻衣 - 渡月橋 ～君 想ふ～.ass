Comment: 0,0:00:00.00,0:00:00.00,JP,,0,0,0,code once,function dyeing(shape,max_n,R) local str={} local tbl={} local pos={} local x1,y1,x2,y2=_G.shape.bounding_real(shape) for i=1,max_n do local x0,y0=math.random(x1,x2),math.random(y1,y2) local s=_G.shape.positive_edge(math.random(30,80),x0,y0,R,0) local s1=_G.shape.intersected(shape,s) local _,num=string.gsub(s1,"[a-z]","") if num>1 then str[#str+1]=s1 local pos1=_G.Yutils.shape.to_pixels(s1) tbl[#tbl+1]=#pos1 _G.table.sort(pos1,function(a,b) local r1,r2=math.pow(a.x-x0,2)+math.pow(a.y-y0,2),math.pow(b.x-x0,2)+math.pow(b.y-y0,2) return r1<r2 end) pos=_G.table.add(pos,pos1) end shape=_G.shape.subtracted(shape,s) end local _,num=string.gsub(shape,"[a-z]","") if num>1 then str[#str+1]=shape local pos1=_G.Yutils.shape.to_pixels(shape) tbl[#tbl+1]=#pos1 _G.table.sort(pos1,function(a,b) local r1,r2=math.pow(a.x,2)+math.pow(a.y,2),math.pow(b.x,2)+math.pow(b.y,2) return r1<r2 end) pos=_G.table.add(pos,pos1) end return pos,tbl,str end
